<h1 class="section-header">
  <i class="icon-envelope"></i>
  Mailings
  <div class="btn-group pull-right">
    <div class="btn btn-view-all">All</div>
    <% if @editable: %>
      <div class="btn btn-add"><i class="icon-plus"></i></div>
    <% end %>
  </div>
</h1>
<table class="table sortable">
  <thead>
    <tr>
      <th>Date</th>
      <th>Subject</th>
      <th>Approved?</th>
    </tr>
  </thead>
  <tbody id="emails">
  </tbody>
</table>
